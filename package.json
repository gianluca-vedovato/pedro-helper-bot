{
  "name": "pedro-bot-node",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "start": "tsx netlify/functions/telegram-webhook-local.ts",
    "build:functions": "rm -rf dist && tsc netlify/functions/*.ts --outDir dist/netlify/functions --target es2020 --module esnext --moduleResolution node --esModuleInterop --allowSyntheticDefaultImports --skipLibCheck --noEmitOnError",
    "lint": "eslint \"**/*.{ts,js}\"",
    "typecheck": "tsc --noEmit",
    "check-webhook": "node check_webhook.js"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.45.4",
    "dotenv": "^17.2.1",
    "openai": "^4.52.0",
    "telegraf": "^4.16.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.5.0",
    "@typescript-eslint/parser": "^8.5.0",
    "eslint": "^9.9.0",
    "tsx": "^4.20.4",
    "typescript": "^5.6.2"
  }
}
